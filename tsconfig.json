{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "lib": ["DOM", "ES2020"],
    "jsx": "react-jsx",
    "moduleResolution": "node",
  "esModuleInterop": true,
  "skipLibCheck": true,
  "strict": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"],
      "@components/*": ["components/*"],
      "@actions/*": ["actions/*"],
      "@types/*": ["types/*"],
      "@lib/*": ["lib/*"],
      "@quibakery/data": ["lib/quibakery-data.ts"],
      "@uibakery/data": ["lib/quibakery-data.ts"]
    },
    "resolveJsonModule": true,
    "noEmit": true
  },
  "include": ["**/*.ts", "**/*.tsx", "types/**/*.d.ts", "src/**/*.d.ts"]
}
